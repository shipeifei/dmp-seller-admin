\"primary\" size=\"medium\" @click=\"$refs.profileCreateEditDialog.open()\">\n        {{ $t('create') }}\n        <i class=\"el-icon-plus el-icon--right\"/>\n      </el-button>\n    </el-card>\n    <el-table :data=\"contents\" class=\"hot-table\">\n      <el-table-column :label=\"$t('profileName')\" prop=\"profileName\"/>\n      <el-table-column :label=\"$t('recommandQuantity')\" prop=\"recommNumber\"/>\n      <el-table-column :label=\"$t('returnType')\" prop=\"returnType\"/>\n      <el-table-column :label=\"$t('profileDes')\" class-name=\"ellipsis\" prop=\"profileDesc\"/>\n      <el-table-column :label=\"$t('action')\">\n        <template slot-scope=\"scope\">\n          <el-button\n            type=\"primary\"\n            icon=\"el-icon-edit\"\n            circle\n            @click=\"itemChange(scope.row, true)\"\n          />\n          <el-button\n            type=\"danger\"\n            icon=\"el-icon-delete\"\n            circle\n            @click=\"itemChange(scope.row, false)\"\n          />\n        </template>\n      </el-table-column>\n    </el-table>\n    <div class=\"block\">\n      <el-pagination\n        :current-page=\"pageIndex\"\n        :page-size=\"pageSize\"\n        :total=\"totalCount\"\n        layout=\"total, prev, pager, next, jumper\"\n        @current-change=\"handleCurrentChange\"\n      />\n    </div>\n    <create-edit-dialog ref=\"profileCreateEditDialog\" @success=\"getPageData\"/>\n  </div>\n</template>\n<script>\nimport createEditDialog from './createEditDialog'\nimport {\n  getProfilePage,\n  getProfile,\n  deleteProfile,\n  saveAndUpdateProfile\n} from '@/http/api/profileApi'\nexport default {\n  name: 'Profile',\n  components: {\n    createEditDialog\n  },\n  data () {\n    return {\n      pageIndex: 1,\n      pageSize: 10,\n      totalCount: 0,\n      contents: []\n    }\n  },\n  computed: {},\n  mounted () {\n    this.getContent(1)\n  },\n  methods: {\n    getContent (pageIndex, pageSize = 10) {\n      getProfilePage({ pageIndex, pageSize }).then(res => {\n        this.totalCount = res.totalCount\n        this.contents = res.contents\n      })\n    },\n    itemChange (...result) {\n      if (result[1]) {\n        let id = result[0].id\n        getProfile(id).then(res => {\n          this.$refs.profileCreateEditDialog.open(res)\n        })\n      } else {\n        this.$confirm(`${this.$t('ConfirmDelete')} \"${result[0].profileName}\"`)\n          .then(() => {\n            deleteProfile(result[0].id).then(() => {\n              this.$message.success(this.$t('ProfileDelSuccessful'))\n              this.getContent(this.pageIndex)\n            })\n          })\n          .catch(() => {\n            this.$message({\n              type: 'info',\n              message: this.$t('CancelDelete')\n            })\n          })\n      }\n    },\n    handleCurrentChange (pageIndex) {\n      this.pageIndex = pageIndex\n      this.getContent(pageIndex)\n    },\n    getPageData (result) {\n      let { data, code } = result\n      saveAndUpdateProfile(data)\n        .then(() => {\n          if (code) {\n            this.getContent(this.pageIndex)\n            this.$message.success(this.$t('ProfileUpdate'))\n          } else {\n            this.pageIndex = 1\n            this.getContent(1)\n            this.$message.success(this.$t('ProfileCreate'))\n          }\n        })\n        .catch(err => {\n          this.$message.error(err.body.message)\n        })\n    }\n  }\n}\n</script>\n<style>\n.ellipsis div.cell {\n  white-space: nowrap;\n}\n</style>\n"],"sourceRoot":"src/views/sys/profile"}]}