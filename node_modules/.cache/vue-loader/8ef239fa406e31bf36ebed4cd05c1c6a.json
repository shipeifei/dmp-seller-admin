-content\">\n      <el-select v-model=\"count\" clearable class=\"dropdown\">\n        <el-option v-for=\"item in 10\" :key=\"item\" :label=\"item\" :value=\"item\"/>\n      </el-select>\n    </el-col>\n\n    <el-col :span=\"9\" class=\"col-content\">\n      <el-date-picker\n        v-model=\"date\"\n        :picker-options=\"setPickerOptions\"\n        :placeholder=\"$t('selectTime')\"\n        class=\"date-picker\"\n        value-format=\"timestamp\"\n        type=\"datetime\"\n      />\n      <span v-if=\"index === 0\" class=\"icon-add\" @click=\"$emit('add')\">+</span>\n      <span v-if=\"index > 0\" class=\"icon-remove\" @click=\"$emit('remove')\">x</span>\n    </el-col>\n  </el-row>\n</template>\n<script>\nimport productList from './productArr'\n\nexport default {\n  props: {\n    index: {\n      type: Number,\n      default: 0\n    }\n  },\n  data () {\n    return {\n      actionType: 'click',\n      actionTypes: ['click', 'collect', 'buy', 'browser'],\n      productId: 94274, // 商品分类\n      count: 1,\n      date: new Date().getTime(),\n      productList,\n      setPickerOptions: {\n        disabledDate (time) {\n          return time.getTime() > Date.now()\n        }\n      }\n    }\n  },\n  methods: {\n    async onActionTypeChange () {\n      this.productId = ''\n      this.count = 1\n      this.date = ''\n    },\n    clear () {\n      this.actionType = ''\n      this.productId = ''\n      this.count = 1\n      this.date = ''\n    },\n    validate () {\n      if (this.actionType === '') {\n        this.$message.error(this.$t('PlesseSelectCategory'))\n        return false\n      }\n      if (this.actionType) {\n        if (!this.productId) {\n          this.$message.error(this.$t('PlesseSelectProduct'))\n          return false\n        }\n        if (this.count === '') {\n          this.$message.error(this.$t('PlesseSelectNumber'))\n          return false\n        }\n        if (!this.date) {\n          this.$message.error(this.$t('PleaseSelectDate'))\n     