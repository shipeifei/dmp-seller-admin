    this.pageIndex = pageIndex\n      if (pageIndex === 1) {\n        this.allTableList = this.tableData.slice(0, this.pageSize)\n      } else {\n        this.allTableList = this.tableData.slice(\n          pageIndex * this.pageSize - this.pageSize,\n          pageIndex * this.pageSize\n        )\n      }\n    }\n  }\n}\n</script>\n<style lang=\"less\" scoped>\n.labelTable {\n  display: flex;\n  flex-direction: column;\n  border: 1px solid #e1e2e5;\n  border-radius: 4px 4px;\n  overflow: hidden;\n  .th {\n      height: 82px;\n      padding: 12px 20px;\n      // background: rgba(0, 134, 237, 0.1);\n      background: rgba(17,44,70,0.2);\n      color: #cae0fd;\n      display: flex;\n      span.paddingLeft {\n        justify-content: flex-start;\n        align-items: center;\n        display: flex;\n        width: 100px;\n        padding-left: 10px;\n        font-size: 12px;\n      }\n      p {\n        display: flex;\n        flex: 1;\n        span {\n          flex: 1;\n          color: #cae0fd;\n          padding-left: 1%;\n          padding-right: 1%;\n          border: none;\n          justify-content: center;\n          display: flex;\n          font-size: 12px;\n        }\n      }\n    }\n  .tableLabelData {\n    display: flex;\n    flex: 1;\n    flex-direction: column;\n    padding: 0 20px;\n    background-color: rgba(17,44,70,0.1);\n    // padding:0 10px;\n    li {\n      align-items: center;\n      display: flex;\n      span.paddingLeft {\n        width: 100px;\n        display: inline-block;\n        padding-left: 10px;\n        font-size: 12px;\n      }\n      p {\n        display: flex;\n        flex: 1;\n        span {\n          flex: 1;\n          color: #bcd2ef;\n          padding-left: 1%;\n          padding-right: 1%;\n          margin-top: 10px;\n          margin-bottom: 10px;\n          border: none;\n          text-align: center;\n          font-size: 12px;\n        }\n      }\n    }\n    .list {\n      color: #bcd2ef;\n      border-bottom: solid 1