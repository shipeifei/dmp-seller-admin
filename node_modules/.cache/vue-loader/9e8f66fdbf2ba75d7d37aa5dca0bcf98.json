rm.enable\" :label=\"true\">{{ $t('enable') }}</el-radio>\n        <el-radio v-model=\"createForm.enable\" :label=\"false\">{{ $t('disenable') }}</el-radio>\n      </el-form-item>\n    </el-form>\n    <el-table :data=\"allAuthorList\" class=\"author-table\">\n      <el-table-column :label=\"$t('permissionName')\">\n        <template slot-scope=\"scope\">\n          <span>{{ $t(scope.row.matchurl) }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column :label=\"$t('action')\">\n        <template slot-scope=\"scope\">\n          <el-checkbox v-model=\"createForm.menuList\" :label=\"scope.row.id\" :key=\"scope.row.id\" />\n        </template>\n      </el-table-column>\n    </el-table>\n  </gucp-dialog>\n</template>\n<script>\nimport { getAuthority } from '@/http/api/authorityApi'\nexport default {\n  data () {\n    return {\n      allAuthorList: [],\n      createForm: {\n        name: '',\n        enable: false,\n        menuList: []\n      }\n    }\n  },\n  methods: {\n    open () {\n      this.createForm = {\n        name: '',\n        enable: false,\n        menuList: []\n      }\n      getAuthority().then(res => {\n        this.allAuthorList = res\n      })\n      this.$refs.createRoleEditDialog.open()\n    },\n    onConfirmCreateType () {\n      if (!this.createForm.name) {\n        this.$message.error(this.$t('RoleName'))\n        return\n      }\n      this.$refs.createRoleEditDialog.close()\n      this.$emit('success', { data: this.createForm, code: 0 })\n    }\n  }\n}\n</script>\n<style lang=\"less\">\n  .author-table .el-checkbox__label {\n    display: none;\n  }\n</style>\n"]}]}